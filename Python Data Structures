#LIST
#List Operations
#Concatenation
l=[1,3,5]
print(l+[2,4,6])

#Repetation
print(["re"]*5)

#List index
a=[1,2,3,4,5]
print(a[2])
print(a[4])

#Slicing
#Syntax: my_list[start:end:step]
l=['a','b','c','d','e','f','g','h','i','j','k','l','m']
print(l)
print(l[1:3])
print(l[1])
print(l[1:-1])
print(l[:3])
print(l[2:])
print(l[-4:-1])
print(l[:-1])
print(l[0:5:2])
print(l[1::2])
print(l[::-1])
print(l[::-2])

#List Methods
#append()
#Syntax: listname.append(element)
